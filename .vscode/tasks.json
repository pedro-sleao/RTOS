{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: avr-gcc build active file",
            "command": "/usr/bin/avr-gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-mmcu=atmega328p",
                "-DF_CPU=16000000UL",
                "-Os"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}